    height: 24px;
    border: 1px solid #c2c2c2;
    text-align: center;
    background: #f1f1f1;
    color: #477fa5;
    margin-left: 8px -webkit-box-shadow: inset 0px 0px 3px #ccc;
    -webkit-box-shadow: inset 0px 0px 3px #ccc;
    -moz-box-shadow: inset 0px 0px 3px #ccc;
    box-shadow: inset 0px 0px 3px #ccc;

}

.bt-count-dateselection {
    clear: both;
}

.bt-statisesms-option {
