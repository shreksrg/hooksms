/* - 日期选择下拉列表*/

.bt-dropdown dd, .bt-dropdown dt, .bt-dropdown ul {
    margin: 0px;
    padding: 0px;
}

.bt-dropdown dd {
    position: relative;
}

.bt-dropdown a, .bt-dropdown a:visited {
    color: #816c5b;
    text-decoration: none;
    outline: none;
}

.bt-dropdown a:hover {
    color: #5d4617;
}

.bt-dropdown dt a:hover {
    color: #5d4617;
    border: 1px solid #d0c9af;
}

.bt-dropdown dt a {
    background: url(img/arrow.png) no-repeat scroll center right;
    display: block;
    padding-right: 18px;
    padding-left: 0.2em;
    border: 1px solid #cccccc;

    -webkit-box-shadow: inset 0px 0px 3px #ccc;
    -moz-box-shadow: inset 0px 0px 3px #ccc;
    box-shadow: inset 0px 0px 3px #ccc;

    -webkit-border-radius: .3em;
    -moz-border-radius: .3em;
    border-radius: .3em
}

.bt-dropdown dt a span {
    cursor: pointer;
    display: block;
    padding: 2px;
}

.bt-dropdown dd ul {
    background: #f5f5f5 none repeat scroll 0 0;
    border: 1px solid #cccccc;
    color: #C5C0B0;
    display: none;
    left: 0px;
    padding: 5px 0px;
    position: absolute;
    top: 2px;
    width: auto;
    min-width: 136px;
    list-style: none;
}

.bt-dropdown dd ul li {
    display: inline-block;
}

.bt-dropdown span.value {
    display: none;
}

.bt-dropdown dd ul li a {
    padding: 5px;
    display: block;
}

.bt-dropdown dd ul li a:hover {
    background-color: #d0c9af;
}

.bt-count-dateselection  ul li {
    display: inline-block;
}

.bt-count-dateselection  ul li.labDate {
    padding: 0 3px;
}

.bt-btn-seeker {
